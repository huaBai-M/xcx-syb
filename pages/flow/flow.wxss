/* pages/flow/flow.wxss */
page {
    height: 100%;
    
}
.pageBody {
    width: 100%;
    min-height: 100%;
    background:#eff5fa;
    /* background: url("http://page-bucket.oiaqye7985.com/wechat/mini-syb/bg.jpg") no-repeat top;
    background-size: cover; */
}
.topTitle {
    margin-top: 30rpx;
}
.table {
  border: 0px solid darkgray;
  margin-top: 40rpx;
  background-color: #fff;
  border-radius: 15rpx;
  overflow: hidden;
  margin-bottom: 30rpx;
  box-shadow: 0 0 30rpx #ccc;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
  border-bottom: 1px solid #ececec;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
    font-size: 28rpx;
    color: #666;
}

.th {
  width: 40%;
  justify-content: center;
  background: #006cff;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.tr:last-child {
    border-bottom: 0;
}
.tiltes {
    text-align: center;
    color: #666;
    font-size: 14px;
    margin: 10rpx 0;
}