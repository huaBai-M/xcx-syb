/* pages/server/index.wxss */
page {
    width: 100%;
    height: 100%;
}
.pageBody {
    width: 100%;
    min-height: 100%;
    background:#eff5fa;
    position: relative;
}
.addRotate {
    width: 100%;
    height: 250rpx;
    background: url("http://page-bucket.oiaqye7985.com/wechat/mini-syb/pic.png") no-repeat center;
    background-size: cover;
}
.topTitle {
    margin-top: -250rpx;
}
.table {
  border: 0px solid darkgray;
  margin-top: 30rpx;
  width: 90%;
  margin-left: 5%;
  background-color: #fff;
  border-radius: 15rpx;
  overflow: hidden;
  margin-bottom: 30rpx;
  /* box-shadow: 0 0 30rpx #ccc; */
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
  border-bottom: 1px solid #ececec;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
    font-size: 28rpx;
    color: #666;
}

.th {
  width: 40%;
  justify-content: center;
  background: #eff5fa;
  color: #006cff;
  display: flex;
  height: 3rem;
  align-items: center;
  
}
.tr:last-child {
    border-bottom: 0;
}
.td text {
    border: 1px solid #e7e7e7;
    border-radius: 20rpx;
    color: #006cff;
    padding: 7rpx 20rpx;
}
.tiltes {
    text-align: center;
    color: #666;
    font-size: 14px;
    margin: 10rpx 0;
}