/**index.wxss**/
@import "../../font/iconfont.wxss";
#mychart-dom-area {
  width: 100%;
  height: 500rpx;
}
#mychart-dom-pie {
  width: 88%;
  height: 600rpx;
  background-color: #fff;
  margin-left: 6%;
  margin-top: -30rpx
}
.pageBody {
    width: 100%;
    background: #eff5fa;
}
.flowH {
	width: 100%;
	height: 165px;
	background: url("http://page-bucket.oiaqye7985.com/wechat/mini-syb/bgHot.png") no-repeat center;
	-webkit-background-size: cover;
	background-size: cover;
    text-align: center;
    padding-top: 50rpx;
}
.flowH .titleF {
	width: 80%;
	margin-left: 10%;
	height: 60rpx;
	line-height: 60rpx;
	border-bottom: 1rpx solid #ccc;
    font-size: 40rpx;
    font-weight: 700;
    color: #000;
    padding-bottom: 20rpx;
}
.flowH  .line {
    display: inline-block;
    height: 30rpx;
    width: 1rpx;
    background-color: #ccc;
    margin: 0 10rpx;
    vertical-align: middle;
}
.flowH .pcM {
	font-size: 30rpx;
	color: #000;
    margin-top: 15rpx;
}
.flowH .pcM .num {
	color: #006cff;
	font-size: 50rpx;
	font-weight: 700;
	margin-left: 10rpx;
	vertical-align: middle;
}
.flowH  button.lookMore {
	height: 35px;
	line-height: 35px;
	background-color: #006cff;
	border: 0;
	margin-top: 40rpx;
	padding: 0 15px;
	width: 50%;
	border-radius: 35rpx;
	color: #fff;
    font-size: 14px;
}
.megS {
	width: 96%;
	margin-left: 2%;
	background-color: #fff;
	border-radius: 30rpx;
	margin-top: 20rpx;
    padding-top: 50rpx;
    padding-bottom: 40rpx;

}
.megS .titleC {
	margin-bottom: 30rpx;
	font-size: 36rpx;
	font-weight: 700;
	color: #000;
}
.megS .dataS {
    text-align: center;
    margin-top: 20rpx;
}
.megS .iline {
    width: 8rpx;
	height: 30rpx;
	background-color: #006cff;
	display: inline-block;
	margin-right:20rpx;
    margin-left: 30rpx;
    vertical-align: middle;
}
.megS .rpoint {
   font-size: 35rpx;
   color: #999;
   float: right;
   margin-right: 30rpx;
}
 .fp {
	margin-top: 20rpx;
    font-size: 54rpx;
    color: #ccc;
    font-weight: 700;
}
.megS .dataS .sP {
	margin-top: 10rpx;
    font-size: 28rpx;
    color: #666;
}
.megS .dataS .data1 {
	position: relative;
}
.megS .dataS  .iRight {
	width: 3rpx;
	height: 80rpx;
	background-color: #e7e7e7;
	display: inline-block;
	position: absolute;
	right: 0;
	top: 15rpx;
}
.container {
    margin-top: 20rpx;
}
.megS .btns {
	text-align: center;
	margin-top: 30rpx;
}
.megS .btns .query{
	width: 50%;
	display: inline-block;
	height: 80rpx;
	line-height: 80rpx;
	background-color: #006cff;
	color: #fff;
	border-radius: 0 0 0 20rpx;
    font-size: 32rpx;
}
.megS .btns .chongzhi{
	width: 50%;
	display: inline-block;
	height: 80rpx;
	line-height: 80rpx;
	background-color: #ffac00;
	color: #fff;
	border-radius: 0 0 20rpx 0;
    font-size: 32rpx;
}
.megS .btns text {
    margin-right: 10rpx;
}
.flowS {
	padding-bottom: 0;
}
.conMap {
    margin-top: 0;
}
.huanTu {
    width: 96%;
    margin-left: 2%;
    border-radius: 20rpx;
    background-color: #fff;
}
.huanTu .items {
    width: 30%;
    background-color: #ccc;
}
.huanTu .titleC {
    width: 100%;
}
.huanTu .titleC .titleSmall{
    margin-left: 58rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #999;
    margin-top: 15rpx;
}
.megS .titleC image {
	width: 62rpx;
    height: 27rpx;
    margin-left: 15rpx;
}
.megS .ranking {
	width: 80%;
	display: block;
	margin-left: 10%;
    margin-bottom: 30rpx;
}
.megS .ranking .rank {
	position: relative;
	text-align: center;
}
.megS .ranking .rank2 {
	text-align: center;
}
.megS .ranking  .iRight {
	width: 5rpx;
	height: 80rpx;
	background-color: #e7e7e7;
	display: inline-block;
	position: absolute;
	right: 0;
	top: 10rpx;
}
.megS .ranking .fp {
    font-size: 54rpx;
    color: #000;
    font-weight: 700; 
    letter-spacing: 2rpx;
}
.megS .ranking .sP {
    font-size: 28rpx;
    color: #666;
    font-weight: 400; 
    margin-top: 10rpx;
}
.megS .progress {
    text-align: center;
    font-size: 28rpx;
    color: #999; 
    margin-top: 50rpx;
}
.megS .paogressP {
    width: 100%;
    height: 16rpx;
    background-color: #e7e7e7;
}
.megS .topP {
    padding-top: 10rpx;
}
.megS .nameP view,
.megS .numP view {
	margin-top: -5px;
}
.megS .shadow {
    height: 100%;
}
.bottomD {
    font-size: 22rpx;
    color: #666;
    text-align: center;
    width: 80%;
    margin-left: 10%;
    margin-top: 25rpx;
    margin-bottom:  25rpx;
}
.van-col--20 {
    margin-left: 8%;
    margin-top: 10rpx;
}
.van-col--10 {
    text-align: left;
    padding-left: 8%;
}
.van-col--12 {
    text-align: right;
}
.mapImage {
    height: 440rpx;
}
.mapImage image{
    width: 100%;
    height: 440rpx;
}
.keyMegs {
    padding-bottom: 60rpx;
}